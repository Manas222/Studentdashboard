<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.student.studentdashboard.StudentDashBoardGraphsFragment">

    <!-- TODO: Update blank fragment layout -->
    <com.jjoe64.graphview.GraphView
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:id="@+id/graph1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <com.jjoe64.graphview.GraphView
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:id="@+id/graph2"
        android:layout_gravity="center_horizontal|top"
        android:layout_below="@+id/graph1"
        android:layout_alignParentStart="true"
        android:layout_marginTop="42dp" />

    <com.jjoe64.graphview.GraphView
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:id="@+id/graph3"
        android:layout_marginTop="48dp"
        android:layout_below="@+id/graph2"
        android:layout_alignParentStart="true" />

    <com.jjoe64.graphview.GraphView
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:id="@+id/graph4"
        android:layout_below="@+id/graph3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="44dp" />

    <com.jjoe64.graphview.GraphView
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:id="@+id/graph5"
        android:layout_below="@+id/graph4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp" />

    <com.jjoe64.graphview.GraphView
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:id="@+id/graph6"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
